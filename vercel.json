{
  "version": 2,
  "name": "claude-code-detail-control-docs",
  "builds": [
    {
      "src": "docs/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/docs/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/docs/$1"
    }
  ],
  "public": true,
  "github": {
    "silent": true
  }
}
